<%= simple_form_for @plan do |f| %>
      <%= f.input :name, as: :url, label: "which name?" %>
      <%= f.input :author, as: :text, label: "which author?" %>
      <%= f.input :img, as: :url, label: "which img?" %>
      <%= f.input :doc, as: :text, label: "which doc?" %>
      <%= f.button :submit, "Add", class: "btn btn-primary" %>
  <% end %>